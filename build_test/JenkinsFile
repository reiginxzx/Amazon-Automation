pipeline{

    agent any

    stages('test'){

        stage('test'){
            steps{
                bat "mvn clean compile test -Dtest=Runner"
            }
        }

        stage('Build application'){
            steps{
                echo "mvn clean install -Dmaven.test.skip-true"
            }
        }

        stage('Create Docker image'){
            steps{
                echo "Creando Docker"
            }
        }


    }

}